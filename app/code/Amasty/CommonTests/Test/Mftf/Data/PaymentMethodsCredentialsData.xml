<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="urn:magento:mftf:DataGenerator/etc/dataProfileSchema.xsd">

    <entity name="paymentMethodCredentialsPayPal" type="paymentMethodsCredentials">
        <data key="emailAssociated">0</data>  <!-- Enter Email Accociated For PayPal-->
        <data key="apiUsername">0</data> <!-- Enter Api Username For PayPal-->
        <data key="apiPassword">0</data> <!-- Enter Api Password For PayPal-->
        <data key="apiSignature">0</data> <!-- Enter Api Signature For PayPal-->
        <data key="sandboxMode">0</data> <!-- 1 is Enable Sandbox Mode, 0 is Disable -->
        <data key="merchantAccountID">0</data> <!-- Enter Merchant Account ID For PayPal-->
    </entity>

    <entity name="paymentMethodCredentialsPayflowPro" type="paymentMethodsCredentials">
        <data key="emailAssociated">0</data> <!-- Enter Email Accociated For Payflow-->
        <data key="partner">0</data> <!-- Enter Partner For Payflow-->
        <data key="user">0</data> <!-- Enter User For Payflow-->
        <data key="vendor">0</data> <!-- Enter Vendor For Payflow-->
        <data key="password">0</data> <!-- Enter Password For Payflow-->
        <data key="testMode">0</data> <!-- 1 is Enable Test Mode, 0 is Disable -->
    </entity>

    <entity name="paymentMethodCredentialsBraintree" type="paymentMethodsCredentials">
        <data key="merchantID">0</data> <!-- Enter Merchant ID For Braintree -->
        <data key="publicKey">0</data> <!-- Enter Public Key For Braintree -->
        <data key="privateKey">0</data> <!-- Enter Private Key For Braintree -->
        <data key="enablePayPal">0</data> <!-- 1 is Enable Pay Pal Braintree, 0 is Disable -->
    </entity>

    <entity name="paymentMethodCredentialsStripe" type="paymentMethodsCredentials">
        <data key="publishableKey">0</data> <!-- Enter Publishable Key For Stripe-->
        <data key="secretKey">0</data> <!-- Enter Secret Key For Stripe-->
        <data key="make3DSecure">0</data> <!-- 1 is Enable 3D Secure, 0 is Disable -->
        <data key="debugMode">0</data> <!-- 1 is Enable Debug Mode, 0 is Disable -->
    </entity>

    <entity name="paymentMethodCredentialsKlarna" type="paymentMethodsCredentials">
        <data key="merchantCountryForTest">0</data> <!-- Enter Merchant Country For Klarna-->
        <data key="merchantCountryOld">0</data> <!-- Enter Old Merchant Country After Testing Klarna-->
        <data key="apiVersion">0</data> <!-- Enter Api Version Value or Name For Klarna-->
        <data key="merchantId">0</data> <!-- Enter Merchant ID For Klarna -->
        <data key="password">0</data> <!-- Enter Password For Klarna -->
        <data key="testModeValue">0</data> <!-- 1 is Enable Test Mode, 0 is Disable -->
        <data key="debugModeValue">0</data> <!-- 1 is Enable Debug Mode, 0 is Disable -->
    </entity>

    <entity name="paymentMethodCredentialsAuthorize" type="paymentMethodsCredentials">
        <data key="apiLoginID">0</data> <!-- Enter Api Login ID For Authorize -->
        <data key="environmentValue">0</data> <!-- Enter Environment Value For Authorize -->
        <data key="transactionKey">0</data> <!-- Enter Transaction Key For Authorize -->
        <data key="publicClientKey">0</data> <!-- Enter Public Client Key For Authorize -->
        <data key="signatureKey">0</data> <!-- Enter Signature Key For Authorize -->
    </entity>

    <entity name="paymentMethodCredentialsEWay" type="paymentMethodsCredentials">
        <data key="sandboxValue">0</data> <!-- 1 is Enable Sandbox Mode, 0 is Disable -->
        <data key="sandboxApiKey">0</data> <!-- Enter Sandbox Api Key For eWay -->
        <data key="sandboxApiPassword">0</data> <!-- Enter Sandbox Api Password For eWay -->
        <data key="sandboxClientEncryptionKey">0</data> <!-- Enter Sandbox Client Encryption Key For eWay -->
    </entity>

    <entity name="paymentMethodCredentialsAmazonPay" type="paymentMethodsCredentials">
        <data key="merchantId">0</data> <!-- Enter Merchant Id For Amazon Pay -->
        <data key="accessId">0</data> <!-- Enter Access Id For Amazon Pay -->
        <data key="secretAccessKey">0</data> <!-- Enter Secret Access Key For Amazon Pay -->
        <data key="clientId">0</data> <!-- Enter Client Id For Amazon Pay -->
        <data key="clientSecret">0</data> <!-- Enter Client Secret For Amazon Pay -->
        <data key="paymentRegionForTest">0</data> <!-- Enter Payment Region For Amazon Pay -->
        <data key="paymentRegionOld">0</data> <!-- Enter Old Payment Region After Testing For Amazon Pay -->
        <data key="sandboxValue">0</data> <!-- 1 is Enable Sandbox Mode, 0 is Disable -->
    </entity>

</entities>